        <!-- Broadcast Receiver that will process AppWidget updates -->
        <receiver
            android:name=".AnimatedWidgetProvider"
            android:label="@string/widget_name">
            <intent-filter>
                <action
                    android:name="android.appwidget.action.APPWIDGET_UPDATE" />
                <action
                    android:name="mobi.intuitit.android.x.launcher.ERROR_FRAME_ANIMATION" />
                <action
                    android:name="mobi.intuitit.android.x.launcher.ERROR_TWEEN_ANIMATION" />
                <action
                    android:name= "mobi.intuitit.android.hpp.ACTION_START_FRAME_ANIMATION"/>
                <action
                    android:name="mobi.intuitit.android.hpp.ACTION_STOP_FRAME_ANIMATION" />
                
                
               
                
                
                
                
            </intent-filter>
            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/widget_animated" />
        </receiver>

    </application>
    <uses-sdk
        android:minSdkVersion="4" />

